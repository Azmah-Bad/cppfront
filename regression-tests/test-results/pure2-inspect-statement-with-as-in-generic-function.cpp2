
print_an_int: ( x: _ ) = {
    std::cout << std::setw(30) << typeid(x).name() 
              << " value is ";
    inspect x {
        is int = std::cout << x as int;
        is _   = std::cout << "not an int";
    }
    std::cout << "\n";
}

main: () -> int = {
    print_an_int("syzygy");
    print_an_int(1);
    print_an_int(1.1);
}
